<?xml version="1.0" ?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="customer_account_navigation">
            <block class="Appseconnect\Shoppinglist\Block\Link\Current"
                   name="customer-account-navigation-shopping-list-link"
                   ifconfig="insync_shoppinglist/general/enable_shoppinglist_active" after="customer-account-navigation-customer-quote-listing">
                <arguments>
                    <argument name="path" xsi:type="string">shoppinglist/customer/account_mylist</argument>
                    <argument name="label" xsi:type="string" translate="true">Shopping Lists</argument>
                    <argument name="sortOrder" xsi:type="number">215</argument>
                </arguments>
            </block>
        </referenceBlock>
    </body>
</page>
