<?xml version="1.0"?>
<page layout="2columns-left" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="customer_account"/>
    <body>
        <referenceBlock name="page.main.title">
            <action method="setPageTitle">
                <argument translate="true" name="title" xsi:type="string">Register New product</argument>
            </action>
        </referenceBlock>
        <referenceContainer name="content">
            <block class="Appseconnect\ServiceRequest\Block\Registerproduct\Newproduct" name="registerproduct.newproduct.block" after="page.top" template="registerproduct/newproduct.phtml" cacheable="false" />
        </referenceContainer>
    </body>
</page>
